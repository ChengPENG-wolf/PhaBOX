library(ggplot2)
library(reshape2)
library(kaos)
wolf = read.csv("phage.csv")
seq <- as.list(strsplit(wolf$seq[1], ""))
seq.cgr <- cgr(seq[[1]], seq.base ="DNA", res = 256)
cgr.plot(seq.cgr, mode = "matrix")
